<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Javid\CustomModel\Block\Uiblock" name="multisel" template="Javid_CustomModel::uidropdown.phtml" cacheable="false" />
            <block class="Magento\Framework\View\Element\Template" name="myfirstcomp" template="Javid_CustomModel::comptemp.phtml">
                <arguments>
                    <argument name="viewModel" xsi:type="object">Javid\CustomModel\ViewModel\CmsPageList</argument>
                </arguments>
            </block>
            <block class="Javid\CustomModel\Block\MyProducts" name="myownprods" template="Javid_CustomModel::myproducts.phtml" />
            <block class="Javid\CustomModel\Block\DependentDropdown" name="depdrop" template="Javid_CustomModel::DependentDropdown.phtml" />
        </referenceContainer>
    </body>
    
</page>